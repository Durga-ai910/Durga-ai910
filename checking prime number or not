#include<stdio.h>
#include<conio.h>
void main()
{
  int i,n,fact=0;
clrscr();
printf("enter a number to check if it is prime or not");
scanf("%d",%n);
for(i=0;i<=n;i++)
{
  if(n%i==0)
{
  fact++;
}
}
if(fact==2)
{
printf("given number is prime");
}
else
{
printf("given number is not prime");
}
getch();
}
